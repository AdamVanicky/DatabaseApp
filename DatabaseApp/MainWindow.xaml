        <TextBox x:Name="tbPersonNumber" HorizontalAlignment="Left" Height="23" Margin="192,130,0,0" TextWrapping="Wrap" Text="Rodné číslo" VerticalAlignment="Top" Width="120"/>

    </Grid>
</Window>
